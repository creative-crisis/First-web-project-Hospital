<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="persistence.Department" table="Departments" schema="public" catalog="Hospital">
        <id name="id">
            <column name="id" sql-type="integer" not-null="true" />
        </id>
        <property name="name">
            <column name="name" sql-type="character varying(75)" not-null="true" />
        </property>
        <property name="chief">
            <column name="chief" sql-type="integer" not-null="true" />
        </property>
    </class>
</hibernate-mapping>